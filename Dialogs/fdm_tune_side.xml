<PropertyList>

   <name>fdm_tune_side</name>
   <modal>false</modal>
   <layout>vbox</layout>
   <width>450</width>
   <height>265</height>
   
   <group>
      <text>
         <label>Tune dat side</label>
         <x>10</x>
         <y>240</y>
      </text>
      
      <!-- slider 1 -->
      
      <text>
         <x>10</x>
         <y>220</y>
         <label>Side_beta</label>
      </text>
      
      <text>
         <x>245</x>
         <y>220</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/side/beta</property>
         <live>true</live>
      </text>
      
      <text>
         <x>400</x>
         <y>220</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/force/Side_beta</property>
         <live>true</live>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>195</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/side/beta</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- slider 2 -->
      
      <text>
         <x>10</x>
         <y>170</y>
         <label>Side_roll_rate</label>
      </text>
      
      <text>
         <x>245</x>
         <y>170</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/side/roll_rate</property>
         <live>true</live>
      </text>
      
      <text>
         <x>400</x>
         <y>170</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/force/Side_roll_rate</property>
         <live>true</live>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>145</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/side/roll_rate</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- slider 3 -->
      
      <text>
         <x>10</x>
         <y>120</y>
         <label>Side_yaw_rate</label>
      </text>
      
      <text>
         <x>245</x>
         <y>120</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/side/yaw_rate</property>
         <live>true</live>
      </text>
      
      <text>
         <x>400</x>
         <y>120</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/force/Side_yaw_rate</property>
         <live>true</live>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>95</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/side/yaw_rate</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- slider 4 -->
      
      <text>
         <x>10</x>
         <y>70</y>
         <label>Side_rudder</label>
      </text>
      
      <text>
         <x>245</x>
         <y>70</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/side/rudder</property>
         <live>true</live>
      </text>
      
      <text>
         <x>400</x>
         <y>70</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/force/Side_rudder</property>
         <live>true</live>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>45</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/side/rudder</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- buttons -->
      
      <button>
         <legend>Apply</legend>
         <y>10</y>
         <x>10</x>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </button>
      
      <button>
         <legend>Close</legend>
         <y>10</y>
         <x>400</x>
         <binding>
            <command>dialog-apply</command>
         </binding>
         <binding>
            <command>dialog-close</command>
         </binding>
      </button>
   
   </group>

</PropertyList>