         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/moment/Pitch_alpha</property>
         <live>true</live>
      </text>

      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>195</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/pitch/alpha</property>
         <min>-2</min>
         <max>2</max>
