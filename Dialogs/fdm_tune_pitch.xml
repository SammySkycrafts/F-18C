         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/moment/Pitch_mach_alpha</property>
         <live>true</live>
      </text>

      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>245</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/pitch/mach-alpha</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>

      <!-- slider 1 -->

      <text>
         <x>10</x>
         <y>220</y>
         <label>Pitch_alpha</label>
      </text>

      <text>
