<PropertyList>
   <name>fdm_tune_pitch</name>
   <modal>false</modal>
   <layout>vbox</layout>
   <width>450</width>
   <height>365</height>
   <group>
      <text>
         <label>Tune dat pitch</label>
         <x>10</x>
         <y>340</y>
      </text>

      <!-- slider -1 -->

      <text>
         <x>10</x>
         <y>320</y>
         <label>Pitch_mach</label>
      </text>

      <text>
         <x>245</x>
         <y>320</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/pitch/mach</property>
         <live>true</live>
      </text>

      <text>
         <x>400</x>
         <y>320</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/moment/Pitch_mach</property>
         <live>true</live>
      </text>

      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>295</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/pitch/mach</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>

      <!-- slider 0 -->

      <text>
         <x>10</x>
         <y>270</y>
         <label>Pitch_mach_alpha</label>
      </text>

      <text>
         <x>245</x>
