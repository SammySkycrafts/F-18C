         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/roll/yaw</property>
         <live>true</live>
      </text>
      
      <text>
         <x>400</x>
         <y>170</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/moment/Roll_yaw</property>
         <live>true</live>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>145</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/roll/yaw</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- slider 4 -->
      
      <text>
         <x>10</x>
         <y>120</y>
         <label>Roll_aileron</label>
      </text>
      
      <text>
         <x>245</x>
         <y>120</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/roll/aileron</property>
         <live>true</live>
      </text>
      
      <text>
         <x>400</x>
         <y>120</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/moment/Roll_aileron</property>
         <live>true</live>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>95</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/roll/aileron</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- slider 5 -->
      
      <text>
         <x>10</x>
         <y>70</y>
         <label>Roll_rudder</label>
      </text>
      
      <text>
         <x>245</x>
         <y>70</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/tune/roll/rudder</property>
         <live>true</live>
      </text>
      
      <text>
         <x>400</x>
         <y>70</y>
         <label>-9.9999</label>
         <format>%-0.4f</format>
         <property>/fdm/jsbsim/aero/moment/Roll_rudder</property>
         <live>true</live>
      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>45</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/roll/rudder</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </slider>
      
      <!-- buttons -->
      
      <button>
         <legend>Apply</legend>
         <y>10</y>
         <x>10</x>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </button>
      
      <button>
         <legend>Close</legend>
         <y>10</y>
         <x>400</x>
         <binding>
            <command>dialog-apply</command>
         </binding>
         <binding>
            <command>dialog-close</command>
         </binding>
      </button>
   
   </group>

</PropertyList>