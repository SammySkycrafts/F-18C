      </text>
      
      <slider>
         <vertical>false</vertical>
         <x>10</x>
         <y>245</y>
         <width>425</width>
         <height>25</height>
         <fraction>0</fraction>
         <property>/fdm/jsbsim/tune/yaw/beta</property>
         <min>-2</min>
         <max>2</max>
         <binding>
            <command>dialog-apply</command>
