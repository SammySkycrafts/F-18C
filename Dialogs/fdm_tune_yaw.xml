      <!-- buttons -->
      
      <button>
         <legend>Apply</legend>
         <y>10</y>
         <x>10</x>
         <binding>
            <command>dialog-apply</command>
         </binding>
      </button>
      
      <button>
         <legend>Close</legend>
         <y>10</y>
         <x>400</x>
         <binding>
            <command>dialog-apply</command>
         </binding>
         <binding>
            <command>dialog-close</command>
         </binding>
      </button>
   
   </group>

</PropertyList>