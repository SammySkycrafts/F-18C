<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <!-- Source   : http://en.wikipedia.org/wiki/MICA_(missile)
                : http://www.aviationsmilitaires.net/display/missile/126

       Picture  : http://www.ausairpower.net/NATO/Matra-MICA-S.jpg
                : http://www.aviationsmilitaires.net/pictures/views/mica.jpg

       Lenght   : 3.10 m
       Diameter : 0.160 m
       Wingspan : 0.560 m
       Weight   : 112 kg (246.91 lb)
  -->

  <path>Aircraft/Mirage-2000/Missiles/MatraMica/MatraMica.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>Mica</object-name>
    <object-name>ailettes</object-name>

    <!-- Objets transparents -->
  </animation>
  <effect>
    <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
    <object-name>Mica</object-name>
    <object-name>ailettes</object-name>
    <condition>
      <not><property>sim/rendering/rembrandt/enabled</property></not>
    </condition>
  </effect>

</PropertyList>

