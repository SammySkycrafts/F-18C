<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <!-- gree navigation lighting -->


  <!-- Turns off non rembrant lighting  -->
  <animation>
    <type>select</type>

    <object-name>GreenLight</object-name>

    <condition>
      <not>
        <property>/sim/rendering/rembrandt/enabled</property>
      </not>
    </condition>
  </animation>

  <model>
    <name>GreenLight</name>
    <path>Aircraft/F-18D/Models/Lights/normal/GreenLight.xml</path>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m> 0 </z-m>
    </offsets>
  </model>


</PropertyList>