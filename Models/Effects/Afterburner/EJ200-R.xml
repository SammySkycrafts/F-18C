<?xml version="1.0" encoding="utf-8"?>

<PropertyList>

  <path>EJ200.ac</path>


  <animation>
    <type>select</type>
    <object-name>ReheatInner</object-name>
    <object-name>ReheatOuter</object-name>
    <condition>
      <greater-than>
        <property>sim/multiplay/generic/float[9]</property>
        <value>0.1</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>blend</type>
    <object-name>ExternalFlame</object-name>
    <property>sim/multiplay/generic/float[9]</property>
    <factor>-0.6</factor>
    <offset>1.0</offset>
    <min>0.0</min>
    <max>1.0</max>
  </animation>

  <animation>
    <type>select</type>
    <object-name>ExternalFlame</object-name>
    <condition>
      <greater-than>
        <property>sim/multiplay/generic/float[9]</property>
        <value>0.4</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>scale</type>
    <object-name>ExternalFlame</object-name>
    <property>sim/multiplay/generic/float[9]</property>
    <x-min>0.2</x-min>
    <y-min>0.8</y-min>
    <z-min>0.8</z-min>
    <x-factor>1
    </x-factor>
    <y-factor>0.1</y-factor>
    <z-factor>0.1</z-factor>
  </animation>

  <animation>
    <object-name>ExternalFlame</object-name>
    <type>spin</type>
    <property>engines/engine[0]/n1</property>
    <factor>340</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>ReheatOuter</object-name>
    <object-name>ReheatInner</object-name>
    <type>spin</type>
    <property>engines/engine[0]/n1</property>
    <factor>437</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>material</type>
    <object-name>ReheatOuter</object-name>
    <emission>
      <red>1</red>
      <green-prop>sim/multiplay/generic/float[9]</green-prop>
      <blue-prop>sim/multiplay/generic/float[9]</blue-prop>
      <factor-prop>sim/multiplay/generic/float[9]</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>ReheatInner</object-name>
    <object-name>ExternalFlame</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor-prop>sim/multiplay/generic/float[9]</factor-prop>
    </emission>
  </animation>

</PropertyList>